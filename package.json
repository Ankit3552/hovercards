{
  "name": "YoCardsExt",
  "description": "",
  "version": "0.13.0",
  "repository": "cameronrohani/YoCardsExt",
  "scripts": {
    "start": "npm run copy:non_js & npm run watchify:background & npm run watchify:everywhere & npm run watchify:sidebar & npm run watchify:soundcloud-player & npm run watchify:top-frame && wait",
    "test": "jshint --reporter=node_modules/jshint-stylish/index.js *.js app/**/*.js",
    "watch:non_js": "watch 'npm run copy:non_js' app",
    "copy:non_js": "rsync -r --exclude=scripts/* app/* dist",
    "watchify:background": "watchify app/scripts/background-main.js -o dist/scripts/background-main.js -r ./app/scripts/development-env.js:env -v",
    "watchify:everywhere": "watchify app/scripts/everywhere-main.js -o dist/scripts/everywhere-main.js -r ./app/scripts/development-env.js:env -v",
    "watchify:sidebar": "watchify app/scripts/sidebar-main.js -o dist/scripts/sidebar-main.js -r ./app/scripts/development-env.js:env -v",
    "watchify:soundcloud-player": "watchify app/scripts/soundcloud-player-main.js -o dist/scripts/soundcloud-player-main.js -r ./app/scripts/development-env.js:env -v",
    "watchify:top-frame": "watchify app/scripts/top-frame-main.js -o dist/scripts/top-frame-main.js -r ./app/scripts/development-env.js:env -v"
  },
  "dependencies": {
    "URIjs": "^1.16.0",
    "YoCardsAPICalls": "cameronrohani/YoCardsAPICalls",
    "angular": "^1.4.3",
    "angular-animate": "^1.4.3",
    "angular-inview": "^1.5.0",
    "angular-messages": "^1.4.3",
    "angular-sanitize": "^1.4.3",
    "async": "^1.4.0",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "moment": "^2.10.5",
    "perfect-scrollbar": "^0.6.4",
    "slick-carousel": "^1.5.8",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^11.0.0",
    "browserify-shim": "^3.8.10",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-concurrent": "^2.0.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "load-grunt-tasks": "^3.2.0",
    "watch": "^0.16.0",
    "watchify": "^3.3.1"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  }
}
